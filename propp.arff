@ATTRIBUTE L2 {L} % Claims of a false hero
@ATTRIBUTE Q {Q, t} % Recognition of the hero | t = transfiguration
@ATTRIBUTE Ex {Ex} % Exposure of the false hero
@ATTRIBUTE T {T, T_1, T_2, T_3, T_4, q} % Transfiguration | q = recognition of hero
@ATTRIBUTE U {U, U_neg} % Punishment of the false hero or villain
@ATTRIBUTE W {W, W_w, w_1, w_2, w_0} % Wedding
@ATTRIBUTE X {X, UwtransX, return} % Unclear or alien forms
@ATTRIBUTE MOVE {MOVE} % transition to another move
